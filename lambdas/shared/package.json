{
  "name": "shared",
  "version": "0.0.1",
  "description": "Code shared between the lambdas.",
  "scripts": {
    "lint": "eslint \"**/*.ts\"",
    "test": "ava",
    "coverage": "nyc --reporter=text --reporter=text-summary ava"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "ava": "^3.15.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "ava": {
    "verbose": true,
    "files": [
      "**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
